@{
    bool isLoginPage = ViewBag.IsLoginPage == true;
}
@if (!isLoginPage)
{
    <environment include="Development">
        <script src="~/lib/vue/dist/vue.js"></script>
        <script src="~/lib/element-ui/lib/index.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://cdn.staticfile.org/vue/2.6.12/vue.min.js"
                asp-fallback-src="~/lib/vue/dist/vue.min.js"
                asp-fallback-test="window.Vue">
        </script>
        <script src="https://cdn.staticfile.org/element-ui/2.15.1/index.min.js"
                asp-fallback-src="~/lib/element-ui/lib/index.js"
                asp-fallback-test="window.Element">
        </script>
    </environment>
}


